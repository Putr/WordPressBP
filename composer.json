{
  "name": "WordPressBP",
  "type": "project",
  "description": "WordPressBP WordPress website",
  "homepage": "https://github.com/andrejcremoznik/WordPressBP",
  "authors": [
    {
      "name": "Andrej Čremožnik",
      "email": "andrej@cremoznik.si",
      "homepage": "https://github.com/andrejcremoznik"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "v1.0.22",
    "vlucas/phpdotenv": "2.1.0",
    "johnpbloch/wordpress": "4.3.1",
    "wpackagist-plugin/disable-emojis": "1.5.1",
    "wpackagist-plugin/timber-library": "0.22.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
